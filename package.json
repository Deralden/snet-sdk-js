{
  "name": "snet-sdk",
  "version": "1.0.1-alpha",
  "description": "SingularityNET SDK for JS",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "snet-sdk",
    "singularitynet-sdk",
    "singularitynet"
  ],
  "scripts": {
    "build": "babel src --out-dir dist",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "lint": "eslint src test",
    "test": "nyc mocha --require @babel/register --colors",
    "test:watch": "mocha --require @babel/register --colors -w"
  },
  "author": "SingularityNET Foundation",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^7.0.0",
    "es6-promise": "^4.2.6",
    "ethereumjs-tx": "^1.3.7",
    "google-protobuf": "^3.7.1",
    "grpc": "^1.19.0",
    "ipfs-http-client": "^30.1.1",
    "lodash": "latest",
    "singularitynet-platform-contracts": "^0.3.0",
    "singularitynet-token-contracts": "^2.0.1",
    "web3": "1.0.0-beta.52",
    "bignumber.js": "^8.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.3"
  }
}
